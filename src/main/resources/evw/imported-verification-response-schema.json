{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://www.gov.uk/evw/imported-verification-response",
  "type": "object",
  "properties": {
    "imported": {
      "type": "array",
      "items": {
        "type" : "string",
        "pattern": "[A-Z\\d]{8}"
      }
    },
    "notImported": {
      "type": "array",
      "items": {
        "type" : "string",
        "pattern": "[A-Z\\d]{8}"
      }
    }
  },
  "additionalProperties": false
}