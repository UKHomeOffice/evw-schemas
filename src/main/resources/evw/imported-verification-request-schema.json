{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://www.gov.uk/evw/imported-verification",
  "type": "object",
  "properties": {
    "applicationReferences": {
      "type": "array",
      "items": {
        "type" : "string",
        "pattern": "[A-Z\\d]{8}"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "applicationReferences"
  ]
}